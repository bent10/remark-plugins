{
  "name": "remark-code-eval-jsx",
  "description": "foo",
  "version": "0.0.0-development",
  "publishConfig": {
    "access": "public"
  },
  "author": "Beni Arisandi (https://stilearning.com)",
  "repository": "https://github.com/bent10/remark-plugins",
  "homepage": "https://github.com/bent10/remark-plugins/tree/main/packages/code-code-eval-jsx",
  "license": "MIT",
  "keywords": [
    "remark",
    "remark-plugin",
    "jsx",
    "react",
    "eval",
    "evaluate",
    "code",
    "codefence"
  ],
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "changelog.md",
    "readme.md"
  ],
  "scripts": {
    "start": "npm run dev",
    "dev": "vite build --watch",
    "build": "vite build --ssr 'src/index.ts' && npm run types",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "types": "tsc src/*.ts -d --emitDeclarationOnly --outDir ./dist",
    "lint": "tsc --noEmit && eslint . --cache --cache-location ../../node_modules/.eslint",
    "format": "prettier . --write --cache-location ../../node_modules/.prettier --ignore-path ../../.prettierignore"
  },
  "dependencies": {
    "unist-util-visit": "^5.0.0"
  }
}
